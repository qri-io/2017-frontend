{
  "presets": [
    ["@babel/preset-env", {
      "targets": { "node": 10 },
      "useBuiltIns": 'usage'
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "add-module-exports", 
    "dynamic-import-webpack",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-classes"
      ]
    },
    "development": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-classes"
      ]
    }
  }
}